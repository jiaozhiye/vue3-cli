{
  "name": "vue3-cli",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development ENV_CONFIG=dev node --max-old-space-size=4096 build/dev.js",
    "dev": "cross-env NODE_ENV=development ENV_CONFIG=dev webpack serve --inline --hot --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node --max-old-space-size=32768 build/prod.js",
    "build-dev": "cross-env NODE_ENV=production ENV_CONFIG=dev node --max-old-space-size=32768 build/prod.js",
    "build-tst": "cross-env NODE_ENV=production ENV_CONFIG=tst node --max-old-space-size=32768 build/prod.js",
    "build-uat": "cross-env NODE_ENV=production ENV_CONFIG=uat node --max-old-space-size=32768 build/prod.js",
    "build-pre": "cross-env NODE_ENV=production ENV_CONFIG=pre node --max-old-space-size=32768 build/prod.js",
    "build-gray": "cross-env NODE_ENV=production ENV_CONFIG=gray node --max-old-space-size=32768 build/prod.js",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "format": "prettier --write \"./src/**/*.{vue,js,ts}\""
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,ts,vue}": [
      "eslint 'src/**/*.{js,ts,vue}'",
      "git add"
    ]
  },
  "dependencies": {
    "add-dom-event-listener": "^1.1.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "element-plus": "^1.0.2-beta.36",
    "js-cookie": "^2.2.1",
    "lodash-es": "^4.17.20",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "screenfull": "^5.1.0",
    "vue": "^3.0.7",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/plugin-proposal-decorators": "^7.12.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.0",
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vue/babel-plugin-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.6",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.8.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.6.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.15.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-vue": "^7.2.0",
    "eslint-webpack-plugin": "^2.5.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.2",
    "mini-css-extract-plugin": "^1.3.6",
    "ora": "^2.1.0",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.0",
    "rimraf": "^2.7.0",
    "sass": "^1.32.0",
    "sass-loader": "^11.0.0",
    "style-resources-loader": "^1.4.0",
    "terser-webpack-plugin": "^5.1.0",
    "thread-loader": "^3.0.1",
    "ts-loader": "^8.0.10",
    "typescript": "^4.1.2",
    "vue-eslint-parser": "^7.5.0",
    "vue-loader": "^16.1.2",
    "vue-style-loader": "^4.1.2",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0",
    "webpack-custom-theme": "^1.0.6",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
